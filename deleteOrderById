--deleteOrderById

CREATE PROC DeleteOrderById
@orderID INT
AS 
BEGIN 
	DELETE 
	FROM [dbo].[Orders]
		WHERE ( [Orders].[OrderID] = @orderID);
	DELETE
	FROM [dbo].[Order Details]
		WHERE ( [Order Details].[OrderID] = @orderID)
END


BEGIN TRY
BEGIN TRAN;
	EXEC DeleteOrderById @orderID = 10248;
COMMIT TRAN;
END TRY
BEGIN CATCH
	ROLLBACK;
END CATCH;	
