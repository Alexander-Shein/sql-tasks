--task б)Создать хранимую процедуру которая принимает OrderID удаляет Order и Order Details по этому ID. (DeleteOrderById)
DROP PROCEDURE DeleteOrderById;

CREATE PROCEDURE DeleteOrderById
@OrderId INTEGER
AS
DELETE
FROM [Order details]
WHERE [OrderId] = @OrderId
DELETE
FROM [Orders]
WHERE [OrderId] = @OrderId;

BEGIN TRY
BEGIN TRAN
EXECUTE DeleteOrderById 10252
COMMIT TRAN
END TRY

BEGIN CATCH

		ROLLBACK TRAN

END CATCH;